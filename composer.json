{
    "name": "alexpts/php-routing",
    "description": "Simple router compatible with the PSR-7",
    "homepage": "https://github.com/alexpts/php-routing",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "router",
        "psr-7"
    ],
    "authors": [{
        "name": "Alexpts",
        "email": "alexpts3@gmail.com"
    }],
    "minimum-stability": "stable",
